{
  "name": "school-result-management-system",
  "version": "1.0.0",
  "description": "A comprehensive school result management system with PostgreSQL database migrations and seeders",
  "main": "index.js",
  "scripts": {
    "db:migrate": "node run-migrations.js",
    "db:migrate:dry": "node run-migrations.js --dry-run",
    "db:migrate:reset": "node run-migrations.js --reset",
    "db:seed": "node run-seeders.js",
    "db:seed:dry": "node run-seeders.js --dry-run",
    "db:seed:reset": "node run-seeders.js --reset",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:setup:reset": "npm run db:migrate:reset && npm run db:seed:reset",
    "db:status": "npm run db:migrate:dry && npm run db:seed:dry",
    "db:specific:seed": "node run-seeders.js --specific",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "school",
    "result",
    "management",
    "system",
    "postgresql",
    "database",
    "migrations",
    "seeders",
    "education",
    "student"
  ],
  "author": "SRMS Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "dotenv": "^17.2.3",
    "mysql2": "^3.15.3",
    "pg": "^8.11.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/school-result-management-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/school-result-management-system/issues"
  },
  "homepage": "https://github.com/your-username/school-result-management-system#readme"
}
